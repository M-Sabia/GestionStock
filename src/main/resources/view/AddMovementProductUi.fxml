                  <Insets left="-9.0" />
               </HBox.margin>
            </Button>
            <ComboBox fx:id="fournisseur_Cb" editable="true" onShowing="#initComboBoxFournisseur" prefWidth="200.0" promptText="Fournisseur..." />
            <Button mnemonicParsing="false" onAction="#addFournisseurPage" text="+">
               <HBox.margin>
                  <Insets left="-9.0" />
               </HBox.margin>
            </Button>
            <TextField fx:id="quantite_Tf" promptText="QuantitÃ©..." />
            <Button mnemonicParsing="false" onAction="#deleteRow" prefWidth="109.0" text="Supprimer" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</AnchorPane>
