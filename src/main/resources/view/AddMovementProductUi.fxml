               <HBox.margin>
                  <Insets left="-9.0" />
               </HBox.margin>
            </Button>
            <ComboBox fx:id="emplacement_Cb" editable="true" onShowing="#initComboBoxLocation" prefWidth="200.0" promptText="Emplacement..." />
