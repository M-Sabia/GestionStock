<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="595.0" prefWidth="1020.0" stylesheets="@../style.css" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0">
         <children>
            <HBox alignment="CENTER" spacing="20.0" VBox.vgrow="ALWAYS">
               <children>
                  <Label text="Choix de l'imprimante :" />
                  <ComboBox fx:id="printer_Cb" onAction="#selectPrinter" onShowing="#refreshPrinterList" prefWidth="300.0" />
                  <JFXButton onAction="#saveSettings" styleClass="generalButton" text="Enregistrer" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
